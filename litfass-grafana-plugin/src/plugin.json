{
  "type": "app",
  "name": "Litfass",
  "id": "mmair-litfass-app",
  "info": {
    "author": {
      "name": "Michael Mair",
      "url": "https://grafana.com"
    },
    "logos": {
      "small": "img/logo.png",
      "large": "img/logo.png"
    },
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
      "name": "Root Page (react)",
      "path": "/a/yourorg-simple-app",
      "role": "Viewer",
      "addToNav": true,
      "defaultNav": true
    },
    {
      "type": "page",
      "name": "Root Page (Tab B)",
      "path": "/a/yourorg-simple-app/?tab=b",
      "role": "Viewer",
      "addToNav": true
    },
    {
      "type": "page",
      "name": "React Config",
      "path": "/plugins/yourorg-simple-app/?page=page2",
      "role": "Admin",
      "addToNav": true
    },
    {
      "type": "dashboard",
      "name": "Streaming Example",
      "path": "dashboards/streaming.json"
    },
    {
      "type": "dashboard",
      "name": "Lots of Stats",
      "path": "dashboards/stats.json"
    },
    {
      "type": "page",
      "name": "Angular Page",
      "component": "AngularExamplePageCtrl",
      "role": "Viewer",
      "addToNav": true
    }
  ],
  "dependencies": {
    "grafanaVersion": "7.2.x",
    "plugins": []
  }
}
